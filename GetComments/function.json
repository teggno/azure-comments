{
  "disabled": false,
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ]
    },
    {
      "name": "commentsTableBinding",
      "type": "table",
      "partitionKey": "{postUrl}",
      "tableName": "Comments",
      "connection": "adw1blogcomments_STORAGE",
      "direction": "in",
      "filter": "public eq true"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ]
}
